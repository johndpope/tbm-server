<h1>Listing Kvstore</h1>

<table style="font-size:10px">
  <thead style="font-size:10px">
    <tr>
      <th>Key1</th>
      <th>Key2</th>
      <th>Value</th>
    </tr>
  </thead>

  <tbody style="font-size:9px">
    <% Kvstore.all.each do |kv| %>
      <tr>
        <td><%= kv.key1 %></td>
        <td><%= kv.key2 %></td>
        <td><%= kv.value %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Delete all kvs', 'kvstore_admin/delete_all' %>
